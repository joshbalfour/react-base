{
  "name": "react-base",
  "version": "0.0.1",
  "description": "react base project",
  "main": "index.js",
  "scripts": {
    "test": "karma start --single-run",
    "start:server": "node dist/server.js",
    "start:server:dev": "nodemon dist/server.js",
    "start:app": "node node_modules/react-native/local-cli/cli.js start",
    "build": "rm -rf dist && NODE_ENV=production npm run build:server && NODE_ENV=production npm run build:client",
    "build:client": "webpack --config webpack/client.js --bail",
    "build:server": "webpack --config webpack/server.js --bail",
    "watch:client": "webpack --watch --config webpack/client.js",
    "watch:server": "webpack --watch --config webpack/server.js",
    "watch": "rm -rf dist && concurrently --raw --kill-others \"npm run watch:client\" \"npm run watch:server\" ",
    "watch:dev": "export NODE_ENV=development && rm -rf dist && concurrently --raw --kill-others \"npm run watch:client\" \"npm run watch:server\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshbalfour/react-base.git"
  },
  "author": "Josh Balfour <josh@joshbalfour.co.uk>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joshbalfour/react-base/issues"
  },
  "homepage": "https://github.com/joshbalfour/react-base#readme",
